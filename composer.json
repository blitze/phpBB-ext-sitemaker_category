{
	"name": "blitze/category",
	"type": "phpbb-extension",
	"description": "Organize your phpBB Sitemaker Content into categories",
	"homepage": "https://github.com/blitze/phpBB-ext-sitemaker_category",
	"version": "3.0.0-dev",
	"time": "2018-02-14 01:01:01",
	"license": "GPL-2.0-only",
	"authors": [{
		"name": "Daniel A.",
		"email": "shizzah@hotmail.com",
		"homepage": "https://github.com/blitze",
		"role": "Lead Developer"
	}],
	"require": {
		"php": ">=5.4",
		"composer/installers": "^1.1",
		"blitze/content": ">=3.0.0-RC4,<4.0.0"
	},
	"require-dev": {
		"phing/phing": "2.4.*"
	},
	"extra": {
		"display-name": "SiteMaker Content Categories",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.2,<3.3.0@dev"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/sitemaker_content_category",
			"filename": "version_check"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
