    "type": "phpbb-extension",
	"description": "Organize your phpBB Sitemaker Content into categories",
	"homepage": "https://github.com/blitze/phpBB-ext-sitemaker_category",
	"version": "3.0.0-dev",
	"time": "2018-02-14 01:01:01",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Daniel A.",
			"email": "shizzah@hotmail.com",
			"homepage": "https://github.com/blitze",
			"role": "Lead Developer"
		}],
	"require": {
		"php": ">=5.4",
		"composer/installers": "^1.1"
	},
	"extra": {
		"display-name": "SiteMaker Content Categories",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.1,<3.3.0@dev"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/sitemaker_category",
			"filename": "version_check"
		}
	}
